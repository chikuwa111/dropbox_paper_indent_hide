{
  "manifest_version": 2,
  "version": "0.2.0",
  "name": "Dropbox Paper Indent Hide",
  "short_name": "Indent Hide",
  "description": "Hide content whose indentation size is larger than specified size.",
  "icons": { "128": "icon_128.png" },
  "content_scripts": [{
    "matches": ["https://paper.dropbox.com/doc/*"],
    "js": ["script.js"]
  }],
  "browser_action": {
    "default_icon": "icon_128.png"
  }
}

